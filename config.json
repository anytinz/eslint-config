    "import/parsers": {
      "espree": [
        ".js",
        ".mjs",
        ".cjs"
      ],
      "@typescript-eslint/parser": [
        ".ts",
        ".mts",
        ".cts"
      ]
    },
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".mjs",
          ".cjs",
          ".json",
          ".ts",
          ".mts",
          ".cts"
        ]
      },
      "typescript": {}
